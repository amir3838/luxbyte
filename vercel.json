{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!public/).*)",
      "destination": "/public/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/dashboard/pharmacy.html"
    }
  ],
  "outputDirectory": "public"
}